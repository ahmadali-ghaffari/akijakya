<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Todos</title>
    <style>
        li {
            font-size: 20px;
        }
    </style>
</head>
<body>
    <h1>Your todos:</h1>
    <ul>
        <% todos.forEach(function(item){ %>
        <li><%= item %></li>
        <% }); %>
    </ul>
</body>
</html>